<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    
    <servlet>
        <servlet-name>ServletFirst</servlet-name>
        <servlet-class>FirstServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PassengerServlet</servlet-name>
        <servlet-class>PassengerServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>TaxistServlet</servlet-name>
        <servlet-class>TaxistServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>RegistrationServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RemoveTaxist</servlet-name>
        <servlet-class>RemoveTaxistServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RefreshLocTaxist</servlet-name>
        <servlet-class>RefreshLocTaxist</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RefreshLocPassenger</servlet-name>
        <servlet-class>RefreshLocPassenger</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Meeting</servlet-name>
        <servlet-class>MeetingServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletFirst</servlet-name>
        <url-pattern>/dir1</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PassengerServlet</servlet-name>
        <url-pattern>/passenger</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>TaxistServlet</servlet-name>
        <url-pattern>/taxist</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RemoveTaxist</servlet-name>
        <url-pattern>/removetx</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RefreshLocTaxist</servlet-name>
        <url-pattern>/refreshtx</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RefreshLocPassenger</servlet-name>
        <url-pattern>/refreshps</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Meeting</servlet-name>
        <url-pattern>/taxinear</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>
